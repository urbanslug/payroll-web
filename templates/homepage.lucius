  float: right;
  background:url("#{profilePic}") no-repeat center center;
  width: 100px;
  height:100px;
  position: relative;
  display: inline-block;
  -webkit-border-radius: 50em;
	-moz-border-radius: 50em;
	border-radius: 50em;
}

.jumbotron {
  margin-top: 50px;
}