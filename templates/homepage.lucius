body {
  color: #9B7D7D;
  background-color: #1A0F0F;
}

table {
  width: 100%;
  text-align: center;
 	border-width: 1px;
  border-color: #666;
}

.per-month{
  display:none;
}

#payroll-container{
  margin-top: 10px;
